{
  "name": "@litert/databar",
  "version": "0.1.0",
  "description": "A simple database connection provider.",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "npm run rebuild",
    "build": "tsc -v && tsc -p .",
    "build-watch": "tsc -v && tsc -w -p .",
    "rebuild": "npm run clean && npm run lint && npm run build",
    "clean": "rm -rf lib examples lib",
    "lint": "tslint -p . -c tslint.json"
  },
  "keywords": [
    "database",
    "connect",
    "pool"
  ],
  "author": "Angus.Fenying <i.am.x.fenying@gmail.com> (https://fenying.net)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/litert/databar.js.git"
  },
  "bugs": {
    "url": "https://github.com/litert/databar.js/issues"
  },
  "homepage": "https://github.com/litert/databar.js#readme",
  "dependencies": {
    "@litert/core": "^1.0.3"
  },
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "devDependencies": {
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^11.12.0",
    "@types/sqlite3": "^3.1.5",
    "mysql2": "^1.6.5",
    "sqlite3": "^4.0.7",
    "typescript": "^3.4.1"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
